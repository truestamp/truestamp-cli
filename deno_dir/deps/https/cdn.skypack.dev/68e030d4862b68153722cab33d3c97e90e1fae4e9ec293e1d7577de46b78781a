import { z } from '/-/zod@v3.18.0-o9af7JvHQqkesyIUExjW/dist=es2019,mode=types/index.d.ts';
export declare const SipHashKeyStruct: z.ZodEffects<z.ZodType<Uint8Array, z.ZodTypeDef, Uint8Array>, Uint8Array, Uint8Array>;
export declare type SipHashKey = z.infer<typeof SipHashKeyStruct>;
export declare const SipHashHashStruct: z.ZodEffects<z.ZodType<Uint8Array, z.ZodTypeDef, Uint8Array>, Uint8Array, Uint8Array>;
export declare type SipHashHash = z.infer<typeof SipHashHashStruct>;
export declare const PayloadStruct: z.ZodObject<{
    test: z.ZodDefault<z.ZodBoolean>;
    timestamp: z.ZodString;
    ulid: z.ZodString;
}, "strip", z.ZodTypeAny, {
    test: boolean;
    timestamp: string;
    ulid: string;
}, {
    test?: boolean | undefined;
    timestamp: string;
    ulid: string;
}>;
export declare type Payload = z.infer<typeof PayloadStruct>;
/**
 * Encodes payload, hashed with SipHash and key, into a string Truestamp ID. Throws an Error if any of the args are invalid.
 *
 * @param payload.test - Whether the ID is a test environment ID.
 * @param payload.timestamp - The timestamp of the ID in microseconds since epoch as a string.
 * @param payload.ulid - The ULID of the ID.
 * @param key - The 8 byte SipHash key.
 * @return - A Truestamp ID string.
 */
export declare const encode: (payload: Payload, key: SipHashKey) => string;
/**
 * Decodes a Truestamp ID string into an ID payload with SipHash verification.
 * Throws an error if the ID is invalid.
 *
 * @param id - A Truestamp ID string.
 * @param key - The 8 byte SipHash key.
 * @return - A decoded ID payload.
 */
export declare const decode: (id: string, key: SipHashKey) => Payload;
/**
 * Validates and decodes a Truestamp ID string unsafely with NO SipHash verification. Not recommended
 * for normal use. Indicates only that the ID has a valid structure. Throws an 'Error' if the ID
 * has an invalid structure.
 *
 * @param id - A Truestamp ID string.
 * @return - ID payload without SipHash verification.
 */
export declare const decodeUnsafely: (id: string) => Payload;
/**
 * Validates a Truestamp ID string with SipHash verification. Does not indicate if the content
 * pointed to by the ID exists, only that it has a valid structure and SipHash.
 *
 * @param id - A Truestamp ID string.
 * @param key - The 8 byte SipHash key.
 * @return - Is the Truestamp ID valid without SipHash verification?
 */
export declare const isValid: (id: string, key: SipHashKey) => boolean;
/**
 * Validates a Truestamp ID string unsafely with NO SipHash verification. Not recommended for
 * normal use. Indicates only that the ID has a valid structure.
 *
 * @param id - A Truestamp ID string.
 * @return - Is the Truestamp ID valid without SipHash verification?
 */
export declare const isValidUnsafely: (id: string) => boolean;
